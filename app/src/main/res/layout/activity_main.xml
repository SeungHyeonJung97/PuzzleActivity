<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/iv_karina001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/karina_001" />

    <ImageView
        android:id="@+id/iv_karina002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintStart_toEndOf="@+id/iv_karina001"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/karina_002" />

    <ImageView
        android:id="@+id/iv_karina003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintStart_toEndOf="@+id/iv_karina002"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/karina_003" />

    <ImageView
        android:id="@+id/iv_karina004"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintStart_toStartOf="@+id/iv_karina001"
        app:layout_constraintTop_toBottomOf="@+id/iv_karina001"
        tools:src="@drawable/karina_004" />

    <ImageView
        android:id="@+id/iv_karina005"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintStart_toEndOf="@+id/iv_karina004"
        app:layout_constraintTop_toBottomOf="@+id/iv_karina002"
        tools:src="@drawable/karina_005" />

    <ImageView
        android:id="@+id/iv_karina006"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintStart_toEndOf="@+id/iv_karina005"
        app:layout_constraintTop_toBottomOf="@+id/iv_karina003"
        tools:src="@drawable/karina_006" />

    <ImageView
        android:id="@+id/iv_karina007"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintStart_toStartOf="@+id/iv_karina004"
        app:layout_constraintTop_toBottomOf="@+id/iv_karina004"
        tools:src="@drawable/karina_007" />

    <ImageView
        android:id="@+id/iv_karina008"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintStart_toEndOf="@+id/iv_karina007"
        app:layout_constraintTop_toBottomOf="@+id/iv_karina005"
        tools:src="@drawable/karina_008" />

    <ImageView
        android:id="@+id/iv_blank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintStart_toEndOf="@+id/iv_karina008"
        app:layout_constraintTop_toBottomOf="@+id/iv_karina006"
        tools:src="@drawable/ic_launcher_background" />

</androidx.constraintlayout.widget.ConstraintLayout>